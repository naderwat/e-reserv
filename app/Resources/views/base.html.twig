       function delete_site(url)
       {
         var response = confirm('Etês vous sûr de vouloir supprimer ce site');
         if(response)
         {

            $.ajax({
                   url : url,
                   Type: "POST",
                   dataType:"JSON",
                   success:function(data)
                   {
                       window.location.href  = window.location;
                   }
            })
         }
       
